#include<iostream>
using namespace std;


class Hero
{
    int health=100;
    public:

    void print()
    {
        cout<<health;
    }

    void sethealth(int h)
    {
        health=h;
    }

    int gethealth()
    {
        return health;
    }

    

    
};

int main()
{
    Hero h1;
    h1.sethealth(50);
    cout<<h1.gethealth()<<endl;
    h1.print();
    return 0;
}
